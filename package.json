{
  "name": "bikelivery-server",
  "version": "2.3.0",
  "description": "Bikelivery",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/main",
    "docker": "cross-env DIR=bikelivery-server docker-compose -f docker-compose.dev.yml -p bikelivery-server up --force-recreate",
    "dev": "cross-env ENV=development nest start --watch",
    "debug": "nest start --debug --watch",
    "test": "cross-env ENV=testing jest --testPathIgnorePatterns test/e2e test/integration --watchAll",
    "test:ci": "cross-env ENV=testing jest --testPathIgnorePatterns test/e2e test/integration",
    "test:integration": "docker-compose -f docker-compose.test.integration.yml up -d && pnpm run test:integration:ci && docker-compose -f docker-compose.test.integration.yml down -v",
    "test:integration:ci": "cross-env ENV=testing jest --testPathPattern='test/integration/.*\\.test\\.ts'",
    "test:e2e": "docker-compose -f docker-compose.test.integration.yml up -d && pnpm run test:e2e:ci && docker-compose -f docker-compose.test.integration.yml down -v",
    "test:e2e:ci": "cross-env ENV=testing jest --testPathPattern='test/e2e/.*\\.test\\.ts'",
    "build": "nest build",
    "typeorm": "pnpm run build && typeorm-ts-node-commonjs -d dist/db/typeorm.config.js",
    "migration:generate": "pnpm run typeorm -- migration:generate",
    "migration:run": "pnpm run typeorm -- migration:run",
    "migration:revert": "pnpm run typeorm -- migration:revert",
    "lint": "eslint .",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js|ts)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.3",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-session": "^2.1.0",
    "core-js": "^3.42.0",
    "dotenv": "^16.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.23"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.24",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
