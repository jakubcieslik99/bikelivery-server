{
  "name": "bikelivery-server",
  "version": "2.1.0",
  "description": "Bikelivery",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/main",
    "docker": "cross-env DIR=bikelivery-server docker-compose -f docker-compose.dev.yml -p bikelivery-server up --force-recreate",
    "dev": "cross-env ENV=development nest start --watch",
    "debug": "nest start --debug --watch",
    "test": "cross-env ENV=testing jest --watchAll",
    "test:ci": "cross-env ENV=testing jest",
    "test:integration": "docker-compose -f docker-compose.test.integration.yml up -d && npm run test:integration:ci && docker-compose -f docker-compose.test.integration.yml down -v",
    "test:integration:ci": "cross-env ENV=testing jest --watchAll",
    "test:e2e": "cross-env ENV=testing jest --watchAll --config ./test/jest-e2e.json",
    "build": "nest build",
    "typeorm": "npm run build && typeorm-ts-node-commonjs -d dist/db/typeorm.config.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "lint": "eslint . --ignore-path ../.gitignore --ext .js,.ts",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js|ts)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-session": "^2.1.0",
    "core-js": "^3.38.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
