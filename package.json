{
  "name": "bikelivery-server",
  "version": "2.0.0",
  "description": "Bikelivery",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/main",
    "docker": "docker-compose -p bikelivery-server up --force-recreate",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "set ENV=testing && jest --watchAll",
    "test:integration": "docker-compose -f docker-compose.test.integration.yml up -d && npm run test:integration:ci && docker-compose -f docker-compose.test.integration.yml down -v",
    "test:integration:ci": "set ENV=testing && jest --watchAll",
    "test:e2e": "set ENV=testing && jest --watchAll --config ./test/jest-e2e.json",
    "build": "nest build",
    "typeorm": "npm run build && typeorm-ts-node-commonjs -d dist/db/typeorm.config.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "lint": "eslint . --ignore-path ../.gitignore --ext .js,.ts",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js|ts)\""
  },
  "author": "Jakub Cie≈õlik",
  "license": "UNLICENSED",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.6.1",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-session": "^2.1.0",
    "core-js": "^3.37.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
